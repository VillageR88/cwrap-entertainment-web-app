{
  "head": {
    "title": "New Candy Wrapper project",
    "link": [
      {
        "rel": "stylesheet",
        "href": "https://fonts.googleapis.com/css2?family=Outfit:wght@100..900&display=swap",
        "type": "text/css"
      }
    ],
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      {
        "charset": "UTF-8"
      },
      {
        "name": "description",
        "content": "New Candy Wrapper project."
      },
      {
        "name": "keywords",
        "content": ""
      }
    ]
  },
  "classroom": [
    {
      "name": "@view-transition",
      "type": "custom",
      "style": "navigation: auto;"
    },
    {
      "name": "placeholder",
      "type": "pseudo::",
      "style": "color: rgba(255,255,255,0.49.79);"
    },
    {
      "name": "html",
      "type": "element",
      "style": "scrollbar-color: #5A698F #10141E;"
    },
    {
      "name": "ul",
      "type": "element",
      "style": "box-sizing: border-box; list-style: none; margin: 0; padding: 0;"
    },
    {
      "name": "nav, li",
      "type": "element",
      "style": "border-radius: 8px; box-sizing: border-box; margin: 0; padding: 0;"
    },
    {
      "name": "a",
      "type": "element",
      "style": "cursor: pointer;"
    },
    {
      "name": "h2",
      "type": "element",
      "style": "color: white; font-size: 2rem; font-weight: 300; letter-spacing: -0.03125rem; margin: 0;"
    },
    {
      "name": "div-title-recommended-ul",
      "type": "id",
      "style": "display: flex; flex-direction: column; gap: 32px;"
    },
    {
      "name": "div-title-recommended-ul > ul",
      "type": "id",
      "style": "display: grid; gap: 16px; gap: 32px 40px; grid-template-columns: repeat(auto-fill, minmax(280px, 1fr)); width: 100%;"
    },
    {
      "name": "div-title-recommended-ul > ul > li",
      "type": "id",
      "style": "align-items: center; border-radius: 8px; display: flex; justify-content: center; width: 100%;"
    },
    {
      "name": "div-title-recommended-ul > ul > li > img",
      "type": "id",
      "style": "border-radius: 8px; display: flex; width: 100%;"
    }
  ],
  "const": {
    "": {
      "placeholder": "Search for movies or TV series"
    },
    "movies": {
      "placeholder": "Search for movies"
    },
    "tv-series": {
      "placeholder": "Search for TV series"
    },
    "bookmarked": {
      "placeholder": "Search for bookmarked shows"
    }
  }
}